{
  "ConnectionStrings": {
    "EventHub": "Eventhub end point",
    "StorageAccount": "Eventhub end point"
  },
  "EventHub": {
    "Name": "mp3eventhub",
    "ConsumerGroup": "$Default",
    "CheckpointContainer": "checkpoints"
  },
  "FileStorage": {
    "OutputDirectory": "ReceivedFiles",
    "CleanupIntervalMinutes": 60,
    "FileRetentionHours": 24
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "EventHubConsumer": "Debug"
    }
  }
}
  